    "test-all": [
      "@phpunit"
    ]
  },
  "autoload": {
    "psr-4": {
      "Poker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  }
}
